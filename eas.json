{
  "cli": {
    "version": ">= 0.52.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "distribution": "store"
    },
    "testflight": {
      "distribution": "internal",
      "ios": {
        "credentialsSource": "remote"
      }
    }
  }
}