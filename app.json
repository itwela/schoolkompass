{
  "expo": {
    "name": "schoolkompass",
    "slug": "schoolkompass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "schoolkompass",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "permissions": [
      "audioRecording"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.itwela.schoolkompass",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "REPLICATE_API_TOKEN_1": "r8_ai4ppQD2FogRc23tuz",
      "REPLICATE_API_TOKEN_2": "aTRw7dptK68VW4gY3lK",
      "eas": {
        "projectId": "92a0f2b7-71e4-44db-8c37-87d956476858"
      }
    }
  }
}
